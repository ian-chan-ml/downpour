<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">


    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="76dp"
        android:layout_marginTop="17dp"
        android:contentDescription="@null"
        app:srcCompat="@drawable/screen_shot_2022_10_26_at_9_19_2" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="318dp"
        android:contentDescription="@null"
        app:srcCompat="@drawable/screen_shot_2022_10_26_at_9_28_1" />


    <TextView
        android:id="@+id/detect_floo"
        android:layout_width="279dp"
        android:layout_height="51dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="53dp"
        android:layout_marginTop="543dp"
        android:gravity="center_horizontal|center_vertical"
        android:lineSpacingExtra="1sp"
        android:text="@string/welcome2_description"
        android:textColor="#000000"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/detect"
        android:layout_width="326dp"
        android:layout_height="80dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="33dp"
        android:layout_marginTop="434dp"
        android:gravity="center_horizontal|center_vertical"
        android:lineSpacingExtra="12sp"
        android:text="@string/welcome2_text"
        android:textSize="25sp"
        android:textColor="#000000"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/skip2"
        android:layout_width="326dp"
        android:layout_height="80dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="33dp"
        android:layout_marginTop="620dp"
        android:gravity="center_horizontal|center_vertical"
        android:lineSpacingExtra="13sp"
        android:text="@string/welcome2_skip"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold" />

    <ImageButton
        android:id="@+id/buttonnextwel2"
        android:layout_width="232dp"
        android:layout_height="48dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="84dp"
        android:layout_marginEnd="77dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/button_bg"
        android:contentDescription="@string/welcome2_next_page"
        android:src="@drawable/_vector_ek55" />


</RelativeLayout>